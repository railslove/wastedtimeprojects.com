<h1>Listing projects</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Url</th>
    <th>Location</th>
    <th>Owner name</th>
    <th>Preview file name</th>
    <th>Preview content type</th>
    <th>Preview file size</th>
    <th>Preview updated at</th>
    <th>Access key</th>
  </tr>

<% for project in @projects %>
  <tr>
    <td><%=h project.name %></td>
    <td><%=h project.description %></td>
    <td><%=h project.url %></td>
    <td><%=h project.location %></td>
    <td><%=h project.owner_name %></td>
    <td><%=h project.preview_file_name %></td>
    <td><%=h project.preview_content_type %></td>
    <td><%=h project.preview_file_size %></td>
    <td><%=h project.preview_updated_at %></td>
    <td><%=h project.access_key %></td>
    <td><%= link_to 'Show', project %></td>
    <td><%= link_to 'Edit', edit_project_path(project) %></td>
    <td><%= link_to 'Destroy', project, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New project', new_project_path %>
